

<form action="/edit" method="put-form">
  <div class="form-group">
    <label for="authEmail">Email</label>
    <input id="authEmail" class="form-control" type="email" name="editEmail">
  </div>

  <div class="form-group">
    <label for="authName">Name</label>
    <input id="authName" class="form-control" type="text" name="editName">
  </div>

  <div class="form-group">
    <label for="authPassword">Password</label>
    <input id="authPassword" class="form-control" type="password" name="editPassword">
  </div>

  <input class="btn btn-success" value="Edit Account" type="editSubmit">
</form>
