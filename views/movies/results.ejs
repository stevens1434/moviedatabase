<link rel="stylesheet" type="text/css" href="../public/css/style.css" />

<h1 class="h1head"><%= results[2].dataValues.name %></h1>


<a href="/">Home</a> |
<a href="/profile">Profile</a> |
<a href="/search">Search</a> |
<a href="/movies">All Movies</a>
<br><br>


<script
  rel="preload"
  src="https://code.jquery.com/jquery-3.2.1.min.js"
  integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
  crossorigin="anonymous"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<script src="../public/js/movies.js"></script>

<%= results[1][6].cast[0].character %>
<br>
<%= results[1][0].poster.large %>


    <div id="manyFound">
    <h4>We found multiple results for that title:</h4> <br>
      <form id="manyForm" action="/search/results" method="GET">
        <% for (var i = 0; i < 6; i++) { %>
          <div id="fh5co-main">
            <div class="container">
              <div class="row">
                <div id="fh5co-board" data-columns>
                  <div class="item">
                    <div class="animate-box">
                    </div>
                      <div class="fh5co-desc">
                            <a href="#" class="image-popup clear fh5co-board-img" title="Photo">
                              <img class="clear"  src="<%= results[1][i].poster.large %>" alt="IMAGE GOES HERE"></a>
                        <div class="clear"  id="subtitle"><strong><a href="<%= results[1][i].url.url %>"><%= results[1][i].title %></a></strong></div>
                        <div class="clear"  id="describe"><br><%= results[1][i].description %></div>
                        <br><p class="clear"> Release Year: <%= results[1][i].year %></p>
                        <p class="clear"> Length: <%= results[1][i].length %> Minutes</p>
                        <p class="clear"> Rating: <%= results[1][i].content_rating %></p>
                          <% for (var j = 0; j < 2; j++) { %>
                             <p class="clear" id="genreList"> <%= results[1][i].genre[j] %></p>,
                          <% } %>
                        <p class="clear"> Score: <%= results[1][i].rating %></p>
                        <br>Actors:
                        <% for (var k = 0; k < 3; k++) { %>
                          <p class="clear"> <%= results[1][i].stars[k] %></p>

                        <% } %>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
            <% } %>
          <br>
        <input id="selectFilmButton" class="btn btn-danger" type="submit" value="Select This Film">
      </form>
    </div>
